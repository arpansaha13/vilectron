{
  "name": "vilectron",
  "packageManager": "pnpm@6.32.3",
  "author": "lemontreejs",
  "description": "Simple Electron-Vite starter template with VueJs and Tailwind",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "scripts": {
    "dev": "vite ./packages/renderer --open",
    "typecheck": "vue-tsc --project packages/renderer/tsconfig.json --noEmit",
    "build": "pnpm typecheck && vite build ./packages/renderer",
    "preview": "vite ./dist/renderer",
    "debug": "pnpm prebuild && vite ./packages/renderer",
    "electron:dev": "node scripts/watch.mjs",
    "electron:build": "pnpm typecheck && node scripts/build.mjs",
    "electron:release": "electron-builder",
    "app:build": "pnpm electron:build && pnpm electron:release"
  },
  "devDependencies": {
    "@headlessui/vue": "^1.5.0",
    "@heroicons/vue": "^1.0.6",
    "@vitejs/plugin-vue": "^2.2.4",
    "@vueuse/core": "^8.0.1",
    "electron": "^17.1.2",
    "electron-builder": "^22.14.13",
    "pinia": "^2.0.12",
    "postcss": "^8.4.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "vite-plugin-pages": "^0.21.4",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.32.1"
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lemontreejs/vilectron.git"
  },
  "bugs": {
    "url": "https://github.com/lemontreejs/vilectron/issues"
  }
}